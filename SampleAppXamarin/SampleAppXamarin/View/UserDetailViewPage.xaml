              xmlns:Template="clr-namespace:SampleAppXamarin.View"
             x:Class="SampleAppXamarin.View.UserDetailViewPage">
    <!--<ContentPage.Content>
         <Template:UserDetailTemplate></Template:UserDetailTemplate>
    </ContentPage.Content>-->
    <ContentPage.Content>
        <ListView x:Name="Userlist"  HasUnevenRows="True" ItemsSource="{Binding UserDetaillist}">
